<!-- insert_top 'div.row.column' -->

<% if paginate_posts.empty? %>
    <div class="callout warning">
        <%= t ".no_post_warning" %>
    </div>
<% end %>
<% if current_participatory_space&.author == current_user && current_participatory_space&.state == "validating" %>
    <div class="callout warning">
        <%= t ".validating_warning" %>
    </div>
<% end %>

